# -*- mode: snippet; require-final-newline: nil; -*-
# name: impl Default
# key: default
# --

impl Default for $1 {
    fn default() -> Self {
        Self {
            $0
        }
    }
}
impl $1 {
    /// Construct a new instance, same as \`$1::default()\`
    pub fn new() -> Self {
        Self::default()$0
    }
}